    "keywords": [
        "cloudflare",
        "api",
        "laravel",
        "artisan"
    ],
    "homepage": "https://github.com/sebdesign/artisan-cloudflare",
    "license": "MIT",
    "authors": [
        {
            "name": "SÃ©bastien Nikolaou",

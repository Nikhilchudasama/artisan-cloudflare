{
    "name": "sebdesign/artisan-cloudflare",
    "description": "Laravel artisan commands for Cloudflare",
    "keywords": [
        "cloudflare",
        "api",
        "laravel",
        "artisan"
    ],
    "homepage": "https://github.com/sebdesign/artisan-cloudflare",
    "license": "MIT",
    "authors": [
        {
            "name": "SÃ©bastien Nikolaou",
            "email": "info@sebdesign.eu",
            "homepage": "https://sebdesign.eu",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^5.5.9|^7.0",
        "guzzlehttp/guzzle": "^6.0",
        "illuminate/console": "5.1.*|5.2.*|5.3.*|5.4.*|5.5.*|5.6.*|5.7.*|5.8.*|^6.0",
        "illuminate/support": "5.1.*|5.2.*|5.3.*|5.4.*|5.5.*|5.6.*|5.7.*|5.8.*|^6.0"
    },
    "require-dev": {
        "mockery/mockery": "^0.9|^1.0",
        "orchestra/testbench": "3.1.*|3.2.*|3.3.*|3.4.*|3.5.*|3.6.*|3.7.*|3.8.*|3.9.*",
        "phpunit/phpunit": "^4.8|^5.0|^6.0|^7.0"
    },
    "autoload": {
        "psr-4": {
            "Sebdesign\\ArtisanCloudflare\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sebdesign\\ArtisanCloudflare\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": "Sebdesign\\ArtisanCloudflare\\ServiceProvider"
        }
    }
}
